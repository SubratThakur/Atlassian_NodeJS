{
  "name": "atlassian-project",
  "version": "1.0.0",
  "description": "Basic NodeJS project setup with Typescript for code design round",
  "main": "dist/index.js",
  "volta": {
    "node": "20.18.2"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "tsc && jest --watchAll",
    "start": "tsc && node dist/index.js",
    "debug": "node --inspect dist/",
    "lint": "eslint . --ext .ts",
    "dev": "tsc && nodemon src/index.ts",
    "single": "tsc && node",
    "solve": "tsc && ts-node-dev --respawn --transpile-only --exit-child ./src/problem-solving/main.ts"
  },
  "keywords": [],
  "author": "Subrat Thakur",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "axios": "^1.6.8",
    "eslint": "^8.57.0",
    "heap": "^0.2.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.2"
  }
}
